<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <shape android:shape="rectangle">
        <corners android:radius="12dp"/>
        <stroke
            android:width="5dp"
            android:color="#ffffbd" /> <!-- Set your desired outline color -->
        <solid android:color="#a8f7e4"/>
        <padding
            android:bottom="2dp"
            android:top="2dp"
            android:left="2dp"
            android:right="2dp" />
    </shape>
</selector>
